{
	"name": "cpsit/cps-myra-cloud",
	"description": "clear cache for MyraCloud",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:CPS-IT/myra-web-api.git"
		}
	],
	"require": {
		"php": "^8.1",
		"ext-json": "*",
		"cpsit/myra-web-api": "^1.0",
		"doctrine/dbal": "^3.0",
		"guzzlehttp/guzzle": "^7.0",
		"psr/http-factory": "^1.0",
		"psr/http-message": "^1.0 || ^2.0",
		"symfony/console": "^6.4 || ^7.0",
		"typo3/cms-backend": "^12.4",
		"typo3/cms-core": "^12.4"
	},
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	},
	"autoload": {
		"psr-4": {
			"CPSIT\\CpsMyraCloud\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CPSIT\\CpsMyraCloud\\Tests\\": "Tests/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "cps_myra_cloud"
		}
	}
}
